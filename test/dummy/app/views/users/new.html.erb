<h1>Register</h1>

<%= form_for @user do |f| %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>    
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>    
  <p>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </p>       
  <p>
    <%= f.submit  %>
  </p>       
<% end %>
  